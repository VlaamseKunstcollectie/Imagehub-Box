# This is inventory file for development stage

# Copy or rename this file from imagehub.default to imagehub
# Change the path-to-machine and let it point to the folder that contains your
# Imagehub-Box instance (i.e. /Users/john/Machines/Imagehub-Box)

[imagehub]
192.168.2.152 ansible_ssh_user='vagrant' ansible_ssh_private_key_file='/path-to-machine/.vagrant/machines/imagehub.box/virtualbox/private_key'

[localhost]
localhost ansible_connection=local

